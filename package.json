{
  "name": "@foo-software/react-feature-toggle",
  "version": "0.0.6",
  "description": "A feature toggle for React",
  "keywords": [
    "react",
    "feature toggle",
    "feature flag",
    "feature switch"
  ],
  "author": "Adam Henson <adamhenson1979@gmail.com>",
  "homepage": "https://github.com/foo-software/react-feature-toggle#readme",
  "license": "MIT",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foo-software/react-feature-toggle.git"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "BABEL_ENV=production babel ./src --out-dir dist",
    "prepare": "npm run clean && npm run build",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "browserslist": ">0.2%, not dead, not op_mini all",
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/register": "^7.4.4",
    "babel-jest": "^24.9.0",
    "husky": "^3.0.1",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "rimraf": "^2.6.3"
  }
}
